<html>
  <head></head>
  <body>
    <h2>Log in</h2>

    <%= form_tag vpsa_user_sessions_path do %>
      <div class="field">
        <%= label_tag :cnpj %><br />
        <%= text_field_tag :cnpj, params[:cnpj] %>
      </div>
      
      <div class="field">
        <%= label_tag :login %><br />
        <%= text_field_tag :login, params[:login] %>
      </div>

      <div class="field">
        <%= label_tag :password %><br />
        <%= password_field_tag :password %>
      </div>

      <div class="actions">
        <%= submit_tag "Log in", class: "button" %>
      </div>
    <% end %>

  </body>
</html>


